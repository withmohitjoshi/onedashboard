{
  "name": "auth",
  "version": "1.0.0",
  "description": "A auth service for authentication and user creation in DB",
  "license": "ISC",
  "author": "Mohit Joshi",
  "type": "module",
  "main": "index.js",
  "sideEffects": false,
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack",
    "build:dev": "cross-env NODE_ENV=development webpack",
    "dev:watch": "webpack --watch",
    "dev:serve": "nodemon",
    "dev": "npm run build:dev && concurrently -n \"webpack,server\" -c \"blue,green\" \"npm run dev:watch\" \"npm run dev:serve\"",
    "prod": "npm run build && cross-env NODE_ENV=production node ./dist/index.js",
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "babel-loader": "^10.0.0",
    "babel-plugin-add-import-extension": "^1.6.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "nodemon": "^3.1.10",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "mongoose": "^8.19.3"
  }
}
