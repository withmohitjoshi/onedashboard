{
  "name": "auth",
  "version": "1.0.0",
  "description": "A auth service for authentication and user creation in DB",
  "license": "ISC",
  "author": "Mohit Joshi",
  "type": "module",
  "main": "index.js",
  "sideEffects": false,
  "scripts": {
    "build:prod": "cross-env NODE_ENV=production webpack",
    "build:dev": "cross-env NODE_ENV=development webpack",
    "dev:webpack": "cross-env NODE_ENV=development webpack --watch",
    "dev:run": "cross-env NODE_ENV=development nodemon ./dist/index.js",
    "prod": "npm run build:prod && cross-env NODE_ENV=production node ./dist/index.js",
    "format": "prettier --write \"src/**/*.{js,json}\"",
    "format:check": "prettier --check \"src/**/*.{js,json}\""
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-env": "^7.28.5",
    "babel-loader": "^10.0.0",
    "babel-plugin-add-import-extension": "^1.6.0",
    "cross-env": "^10.1.0",
    "nodemon": "^3.1.10",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "dotenv": "^17.2.3",
    "express": "^5.1.0"
  }
}
